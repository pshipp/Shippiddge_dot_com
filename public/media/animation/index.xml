      <guid>/media/animation/animation00/</guid>
      <description>&lt;h1  style=&#34;text-align:center; font-family: &#39;Lucida Console&#39;&#34;&gt;ANIMATION&lt;/h1&gt;&#xD;&#xA;&lt;!-- Big Video Player --&gt;&#xD;&#xA;&lt;div class=&#34;big-player-container my-5&#34; id=&#34;bigPlayer&#34;&gt;&#xD;&#xA;  &lt;div class=&#34;ratio ratio-16x9&#34;&gt;&#xD;&#xA;    &lt;iframe id=&#34;mainVideo&#34; &#xD;&#xA;            src=&#34;https://www.youtube.com/embed/y1pcriNPUr0&#34;&#xD;&#xA;            title=&#34;Featured Video&#34;&#xD;&#xA;            allowfullscreen&#xD;&#xA;            loading=&#34;lazy&#34;&gt;&#xD;&#xA;    &lt;/iframe&gt;&#xD;&#xA;  &lt;/div&gt;&#xD;&#xA;&lt;/div&gt;&#xD;&#xA;&#xD;&#xA;&lt;!-- Navigation Controls - Centered --&gt;&#xD;&#xA;&lt;div style=&#34;display: flex; justify-content: center; align-items: center; margin-top: 1rem; margin-bottom: 1.5rem; gap: 1rem;&#34;&gt;&#xD;&#xA;  &lt;button id=&#34;prevBtn&#34; class=&#34;nav-button btn&#34; onclick=&#34;navigateVideo(-1)&#34; style=&#34;padding: 10px 20px;&#34;&gt;&#xD;&#xA;    ← Previous&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;  &#xD;&#xA;  &lt;div style=&#34;text-align: center; min-width: 200px;&#34;&gt;&#xD;&#xA;    &lt;span id=&#34;videoTitle&#34; style=&#34;display: block; font-weight: 500;&#34;&gt;Featured Video&lt;/span&gt;&#xD;&#xA;    &lt;div style=&#34;font-size: 0.875rem; margin-top: 0.25rem;&#34;&gt;&#xD;&#xA;      &lt;span id=&#34;videoCounter&#34;&gt;1 / 1&lt;/span&gt;&#xD;&#xA;    &lt;/div&gt;&#xD;&#xA;  &lt;/div&gt;&#xD;&#xA;  &#xD;&#xA;  &lt;button id=&#34;nextBtn&#34; class=&#34;nav-button btn&#34; onclick=&#34;navigateVideo(1)&#34; style=&#34;padding: 10px 20px;&#34;&gt;&#xD;&#xA;    Next →&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;&lt;/div&gt;&#xD;&#xA;&#xD;&#xA;&lt;!-- UNIFIED MASTER SCRIPT --&gt;&#xD;&#xA;&lt;script&gt;&#xD;&#xA;  // Video playlist management&#xD;&#xA;  let videoPlaylist = [];&#xD;&#xA;  let currentVideoIndex = -1; // Start at -1 to indicate no video selected from playlist yet&#xD;&#xA;  const defaultVideoId = &#39;zgUFaThdFG8&#39;; // The featured video ID&#xD;&#xA;  &#xD;&#xA;  // Play a specific video&#xD;&#xA;  function playVideo(videoId, platform, title) {&#xD;&#xA;    platform = platform || &#39;youtube&#39;;&#xD;&#xA;    const iframe = document.getElementById(&#39;mainVideo&#39;);&#xD;&#xA;    &#xD;&#xA;    let embedUrl;&#xD;&#xA;    if (platform === &#39;dtube&#39;) {&#xD;&#xA;      embedUrl = `https://play.d.tube?v=${videoId}`;&#xD;&#xA;    } else {&#xD;&#xA;      embedUrl = `https://www.youtube.com/embed/${videoId}?autoplay=1`;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    iframe.src = embedUrl;&#xD;&#xA;    &#xD;&#xA;    // Update video info if title is provided&#xD;&#xA;    if (title) {&#xD;&#xA;      document.getElementById(&#39;videoTitle&#39;).textContent = title;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    // Update navigation buttons&#xD;&#xA;    updateNavigationButtons();&#xD;&#xA;    &#xD;&#xA;    // Scroll to player&#xD;&#xA;    document.getElementById(&#39;bigPlayer&#39;).scrollIntoView({&#xD;&#xA;      behavior: &#39;smooth&#39;,&#xD;&#xA;      block: &#39;center&#39;&#xD;&#xA;    });&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;  // Navigate between videos&#xD;&#xA;  function navigateVideo(direction) {&#xD;&#xA;    if (videoPlaylist.length === 0) return;&#xD;&#xA;    &#xD;&#xA;    // If we&#39;re on the default video (currentVideoIndex is -1), go to first or last&#xD;&#xA;    if (currentVideoIndex === -1) {&#xD;&#xA;      currentVideoIndex = direction &gt; 0 ? 0 : videoPlaylist.length - 1;&#xD;&#xA;    } else {&#xD;&#xA;      currentVideoIndex += direction;&#xD;&#xA;      &#xD;&#xA;      // Wrap around&#xD;&#xA;      if (currentVideoIndex &gt;= videoPlaylist.length) {&#xD;&#xA;        currentVideoIndex = 0;&#xD;&#xA;      } else if (currentVideoIndex &lt; 0) {&#xD;&#xA;        currentVideoIndex = videoPlaylist.length - 1;&#xD;&#xA;      }&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    const video = videoPlaylist[currentVideoIndex];&#xD;&#xA;    playVideo(video.id, video.platform, video.title);&#xD;&#xA;    &#xD;&#xA;    // Update active button&#xD;&#xA;    updateActiveButton();&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;  // Update navigation button states&#xD;&#xA;  function updateNavigationButtons() {&#xD;&#xA;    const prevBtn = document.getElementById(&#39;prevBtn&#39;);&#xD;&#xA;    const nextBtn = document.getElementById(&#39;nextBtn&#39;);&#xD;&#xA;    const counter = document.getElementById(&#39;videoCounter&#39;);&#xD;&#xA;    &#xD;&#xA;    if (videoPlaylist.length &lt;= 1) {&#xD;&#xA;      prevBtn.disabled = true;&#xD;&#xA;      nextBtn.disabled = true;&#xD;&#xA;    } else {&#xD;&#xA;      prevBtn.disabled = false;&#xD;&#xA;      nextBtn.disabled = false;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    // Update counter - show actual position or &#34;Featured&#34; if on default video&#xD;&#xA;    if (currentVideoIndex === -1) {&#xD;&#xA;      counter.textContent = `Featured / ${videoPlaylist.length}`;&#xD;&#xA;    } else {&#xD;&#xA;      counter.textContent = `${currentVideoIndex + 1} / ${videoPlaylist.length}`;&#xD;&#xA;    }&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;  // Update active button highlight&#xD;&#xA;  function updateActiveButton() {&#xD;&#xA;    document.querySelectorAll(&#39;.video-btn&#39;).forEach((btn, index) =&gt; {&#xD;&#xA;      if (index === currentVideoIndex) {&#xD;&#xA;        btn.classList.add(&#39;active&#39;);&#xD;&#xA;      } else {&#xD;&#xA;        btn.classList.remove(&#39;active&#39;);&#xD;&#xA;      }&#xD;&#xA;    });&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;  // Initialize video buttons and build playlist&#xD;&#xA;  function initializeVideoButtons() {&#xD;&#xA;    const buttons = document.querySelectorAll(&#39;.video-btn&#39;);&#xD;&#xA;    videoPlaylist = [];&#xD;&#xA;    &#xD;&#xA;    buttons.forEach((btn, index) =&gt; {&#xD;&#xA;      const videoId = btn.getAttribute(&#39;data-video-id&#39;);&#xD;&#xA;      const platform = btn.getAttribute(&#39;data-platform&#39;) || &#39;youtube&#39;;&#xD;&#xA;      const title = btn.getAttribute(&#39;data-title&#39;) || btn.textContent.trim();&#xD;&#xA;      &#xD;&#xA;      videoPlaylist.push({ id: videoId, platform: platform, title: title });&#xD;&#xA;      &#xD;&#xA;      btn.addEventListener(&#39;click&#39;, function() {&#xD;&#xA;        currentVideoIndex = index;&#xD;&#xA;        playVideo(videoId, platform, title);&#xD;&#xA;        updateActiveButton();&#xD;&#xA;      });&#xD;&#xA;    });&#xD;&#xA;    &#xD;&#xA;    // Don&#39;t auto-select first video since we have a featured default&#xD;&#xA;    // Just update the navigation buttons&#xD;&#xA;    updateNavigationButtons();&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;  // Keyboard navigation&#xD;&#xA;  document.addEventListener(&#39;keydown&#39;, function(e) {&#xD;&#xA;    if (e.key === &#39;ArrowLeft&#39;) {&#xD;&#xA;      navigateVideo(-1);&#xD;&#xA;    } else if (e.key === &#39;ArrowRight&#39;) {&#xD;&#xA;      navigateVideo(1);&#xD;&#xA;    }&#xD;&#xA;  });&#xD;&#xA;  &#xD;&#xA;  // Initialize when DOM is ready&#xD;&#xA;  if (document.readyState === &#39;loading&#39;) {&#xD;&#xA;    document.addEventListener(&#39;DOMContentLoaded&#39;, initializeVideoButtons);&#xD;&#xA;  } else {&#xD;&#xA;    initializeVideoButtons();&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;  // Re-initialize whenever new content is loaded&#xD;&#xA;  window.reinitVideoButtons = initializeVideoButtons;&#xD;&#xA;&lt;/script&gt;</description>
