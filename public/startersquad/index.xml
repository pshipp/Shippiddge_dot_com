    <item>
      <title></title>
      <link>/startersquad/startersquad00/</link>
      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      <guid>/startersquad/startersquad00/</guid>
      <description>&lt;body&gt;&#xD;&#xA;&lt;h1 style=&#34;text-align:center; font-family: &#39;Lucida Console&#39;&#34;&gt;STARTER SQUAD&lt;/h1&gt;&#xD;&#xA;&lt;p style=&#34;text-align:center&#34;&gt; &#xD;&#xA;&#xD;&#xA; &lt;b&gt;Starter Squad&lt;/b&gt; is an animated parody series starring the original starter Pokemon on their quest for evolution.  &#xD;&#xA;  &#xD;&#xA;  &lt;br&gt;The series has been produced at a slow drip production cycle since 2013, with each episode attempting to out-do the last.  &#xD;&#xA;    Once completed, the series will consist of &lt;b&gt;twelve&lt;/b&gt; episodes.&#xD;&#xA;    Interested in creating a language dub?  Check out the &lt;a href=&#34;/startersquad/dubbing&#34;&gt;Official Dubbing Pack&lt;/a&gt;.&#xD;&#xA;  &#xD;&#xA;  &#xD;&#xA;  &lt;p&gt;&#xD;&#xA;  &lt;/body&gt;&#xD;&#xA;&#xD;&#xA;&lt;!-- Big Video Player --&gt;&#xD;&#xA;&lt;div class=&#34;big-player-container my-5&#34; id=&#34;bigPlayer&#34;&gt;&#xD;&#xA;  &lt;div class=&#34;ratio ratio-16x9&#34;&gt;&#xD;&#xA;    &lt;iframe id=&#34;mainVideo&#34; &#xD;&#xA;            src=&#34;https://www.youtube.com/embed/4x_lObzaD2w&#34;&#xD;&#xA;            title=&#34;Featured Video&#34;&#xD;&#xA;            allowfullscreen&#xD;&#xA;            loading=&#34;lazy&#34;&gt;&#xD;&#xA;    &lt;/iframe&gt;&#xD;&#xA;  &lt;/div&gt;&#xD;&#xA;&lt;/div&gt;&#xD;&#xA;&#xD;&#xA;&lt;!-- Navigation Controls - Centered --&gt;&#xD;&#xA;&lt;div style=&#34;display: flex; justify-content: center; align-items: center; margin-top: 1rem; margin-bottom: 1.5rem; gap: 1rem;&#34;&gt;&#xD;&#xA;  &lt;button id=&#34;prevBtn&#34; class=&#34;nav-button btn&#34; onclick=&#34;navigateVideo(-1)&#34; style=&#34;padding: 10px 20px;&#34;&gt;&#xD;&#xA;    ← Previous&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;  &#xD;&#xA;  &lt;div style=&#34;text-align: center; min-width: 200px;&#34;&gt;&#xD;&#xA;    &lt;span id=&#34;videoTitle&#34; style=&#34;display: block; font-weight: 500;&#34;&gt;Featured Video&lt;/span&gt;&#xD;&#xA;    &lt;div style=&#34;font-size: 0.875rem; margin-top: 0.25rem;&#34;&gt;&#xD;&#xA;      &lt;span id=&#34;videoCounter&#34;&gt;1 / 1&lt;/span&gt;&#xD;&#xA;    &lt;/div&gt;&#xD;&#xA;  &lt;/div&gt;&#xD;&#xA;  &#xD;&#xA;  &lt;button id=&#34;nextBtn&#34; class=&#34;nav-button btn&#34; onclick=&#34;navigateVideo(1)&#34; style=&#34;padding: 10px 20px;&#34;&gt;&#xD;&#xA;    Next →&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;&lt;/div&gt;&#xD;&#xA;&#xD;&#xA;&lt;!-- UNIFIED MASTER SCRIPT --&gt;&#xD;&#xA;&lt;script&gt;&#xD;&#xA;  // Video playlist management&#xD;&#xA;  let videoPlaylist = [];&#xD;&#xA;  let currentVideoIndex = -1; // Start at -1 to indicate no video selected from playlist yet&#xD;&#xA;  const defaultVideoId = &#39;zgUFaThdFG8&#39;; // The featured video ID&#xD;&#xA;  &#xD;&#xA;  // Play a specific video&#xD;&#xA;  function playVideo(videoId, platform, title) {&#xD;&#xA;    platform = platform || &#39;youtube&#39;;&#xD;&#xA;    const iframe = document.getElementById(&#39;mainVideo&#39;);&#xD;&#xA;    &#xD;&#xA;    let embedUrl;&#xD;&#xA;    if (platform === &#39;dtube&#39;) {&#xD;&#xA;      embedUrl = `https://play.d.tube?v=${videoId}`;&#xD;&#xA;    } else {&#xD;&#xA;      embedUrl = `https://www.youtube.com/embed/${videoId}?autoplay=1`;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    iframe.src = embedUrl;&#xD;&#xA;    &#xD;&#xA;    // Update video info if title is provided&#xD;&#xA;    if (title) {&#xD;&#xA;      document.getElementById(&#39;videoTitle&#39;).textContent = title;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    // Update navigation buttons&#xD;&#xA;    updateNavigationButtons();&#xD;&#xA;    &#xD;&#xA;    // Scroll to player&#xD;&#xA;    document.getElementById(&#39;bigPlayer&#39;).scrollIntoView({&#xD;&#xA;      behavior: &#39;smooth&#39;,&#xD;&#xA;      block: &#39;center&#39;&#xD;&#xA;    });&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;  // Navigate between videos&#xD;&#xA;  function navigateVideo(direction) {&#xD;&#xA;    if (videoPlaylist.length === 0) return;&#xD;&#xA;    &#xD;&#xA;    // If we&#39;re on the default video (currentVideoIndex is -1), go to first or last&#xD;&#xA;    if (currentVideoIndex === -1) {&#xD;&#xA;      currentVideoIndex = direction &gt; 0 ? 0 : videoPlaylist.length - 1;&#xD;&#xA;    } else {&#xD;&#xA;      currentVideoIndex += direction;&#xD;&#xA;      &#xD;&#xA;      // Wrap around&#xD;&#xA;      if (currentVideoIndex &gt;= videoPlaylist.length) {&#xD;&#xA;        currentVideoIndex = 0;&#xD;&#xA;      } else if (currentVideoIndex &lt; 0) {&#xD;&#xA;        currentVideoIndex = videoPlaylist.length - 1;&#xD;&#xA;      }&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    const video = videoPlaylist[currentVideoIndex];&#xD;&#xA;    playVideo(video.id, video.platform, video.title);&#xD;&#xA;    &#xD;&#xA;    // Update active button&#xD;&#xA;    updateActiveButton();&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;  // Update navigation button states&#xD;&#xA;  function updateNavigationButtons() {&#xD;&#xA;    const prevBtn = document.getElementById(&#39;prevBtn&#39;);&#xD;&#xA;    const nextBtn = document.getElementById(&#39;nextBtn&#39;);&#xD;&#xA;    const counter = document.getElementById(&#39;videoCounter&#39;);&#xD;&#xA;    &#xD;&#xA;    if (videoPlaylist.length &lt;= 1) {&#xD;&#xA;      prevBtn.disabled = true;&#xD;&#xA;      nextBtn.disabled = true;&#xD;&#xA;    } else {&#xD;&#xA;      prevBtn.disabled = false;&#xD;&#xA;      nextBtn.disabled = false;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    // Update counter - show actual position or &#34;Featured&#34; if on default video&#xD;&#xA;    if (currentVideoIndex === -1) {&#xD;&#xA;      counter.textContent = `Featured / ${videoPlaylist.length}`;&#xD;&#xA;    } else {&#xD;&#xA;      counter.textContent = `${currentVideoIndex + 1} / ${videoPlaylist.length}`;&#xD;&#xA;    }&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;  // Update active button highlight&#xD;&#xA;  function updateActiveButton() {&#xD;&#xA;    document.querySelectorAll(&#39;.video-btn&#39;).forEach((btn, index) =&gt; {&#xD;&#xA;      if (index === currentVideoIndex) {&#xD;&#xA;        btn.classList.add(&#39;active&#39;);&#xD;&#xA;      } else {&#xD;&#xA;        btn.classList.remove(&#39;active&#39;);&#xD;&#xA;      }&#xD;&#xA;    });&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;  // Initialize video buttons and build playlist&#xD;&#xA;  function initializeVideoButtons() {&#xD;&#xA;    const buttons = document.querySelectorAll(&#39;.video-btn&#39;);&#xD;&#xA;    videoPlaylist = [];&#xD;&#xA;    &#xD;&#xA;    buttons.forEach((btn, index) =&gt; {&#xD;&#xA;      const videoId = btn.getAttribute(&#39;data-video-id&#39;);&#xD;&#xA;      const platform = btn.getAttribute(&#39;data-platform&#39;) || &#39;youtube&#39;;&#xD;&#xA;      const title = btn.getAttribute(&#39;data-title&#39;) || btn.textContent.trim();&#xD;&#xA;      &#xD;&#xA;      videoPlaylist.push({ id: videoId, platform: platform, title: title });&#xD;&#xA;      &#xD;&#xA;      btn.addEventListener(&#39;click&#39;, function() {&#xD;&#xA;        currentVideoIndex = index;&#xD;&#xA;        playVideo(videoId, platform, title);&#xD;&#xA;        updateActiveButton();&#xD;&#xA;      });&#xD;&#xA;    });&#xD;&#xA;    &#xD;&#xA;    // Don&#39;t auto-select first video since we have a featured default&#xD;&#xA;    // Just update the navigation buttons&#xD;&#xA;    updateNavigationButtons();&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;  // Keyboard navigation&#xD;&#xA;  document.addEventListener(&#39;keydown&#39;, function(e) {&#xD;&#xA;    if (e.key === &#39;ArrowLeft&#39;) {&#xD;&#xA;      navigateVideo(-1);&#xD;&#xA;    } else if (e.key === &#39;ArrowRight&#39;) {&#xD;&#xA;      navigateVideo(1);&#xD;&#xA;    }&#xD;&#xA;  });&#xD;&#xA;  &#xD;&#xA;  // Initialize when DOM is ready&#xD;&#xA;  if (document.readyState === &#39;loading&#39;) {&#xD;&#xA;    document.addEventListener(&#39;DOMContentLoaded&#39;, initializeVideoButtons);&#xD;&#xA;  } else {&#xD;&#xA;    initializeVideoButtons();&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;  // Re-initialize whenever new content is loaded&#xD;&#xA;  window.reinitVideoButtons = initializeVideoButtons;&#xD;&#xA;&lt;/script&gt;</description>
    </item>
    <item>
      <title></title>
      <link>/startersquad/startersquad01/</link>
      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      <guid>/startersquad/startersquad01/</guid>
      <description>&lt;style&gt;&#xD;&#xA;  /* Grid Layout: &#xD;&#xA;     - 1 column on mobile&#xD;&#xA;     - 2 columns on tablets&#xD;&#xA;     - 4 columns on desktop &#xD;&#xA;     - No external Bootstrap required (Theme Safe)&#xD;&#xA;  */&#xD;&#xA;  .starter-grid {&#xD;&#xA;    display: grid;&#xD;&#xA;    grid-template-columns: repeat(1, 1fr);&#xD;&#xA;    gap: 1.5rem;&#xD;&#xA;    margin-top: 2rem;&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  @media (min-width: 768px) {&#xD;&#xA;    .starter-grid { grid-template-columns: repeat(2, 1fr); }&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  @media (min-width: 1024px) {&#xD;&#xA;    .starter-grid { grid-template-columns: repeat(4, 1fr); }&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  /* Button Styling */&#xD;&#xA;  .video-btn {&#xD;&#xA;    width: 100%;&#xD;&#xA;    border: 0;&#xD;&#xA;    padding: 0;&#xD;&#xA;    cursor: pointer;&#xD;&#xA;    border-radius: 8px;&#xD;&#xA;    overflow: hidden;&#xD;&#xA;    background: #212529; /* Dark background to match dark mode */&#xD;&#xA;    box-shadow: 0 4px 6px rgba(0,0,0,0.3);&#xD;&#xA;    transition: transform 0.2s;&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  .video-btn:hover {&#xD;&#xA;    transform: scale(1.02);&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  .video-btn img {&#xD;&#xA;    width: 100%;&#xD;&#xA;    display: block;&#xD;&#xA;    height: auto;&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  .video-btn div {&#xD;&#xA;    padding: 1rem;&#xD;&#xA;    text-align: left;&#xD;&#xA;    color: white;&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  .video-btn h5 {&#xD;&#xA;    margin: 0;&#xD;&#xA;    font-size: 1rem;&#xD;&#xA;    font-weight: 500;&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  /* Mystery Placeholder for Ep 12 */&#xD;&#xA;  .mystery-btn {&#xD;&#xA;    background: #333;&#xD;&#xA;    cursor: default;&#xD;&#xA;    opacity: 0.7;&#xD;&#xA;    display: flex;&#xD;&#xA;    flex-direction: column;&#xD;&#xA;  }&#xD;&#xA;  &#xD;&#xA;  .mystery-btn:hover {&#xD;&#xA;    transform: none;&#xD;&#xA;  }&#xD;&#xA;&lt;/style&gt;&#xD;&#xA;&#xD;&#xA;&lt;h3 class=&#34;mb-4&#34;&gt;EPISODES&lt;/h3&gt;&#xD;&#xA;&lt;div class=&#34;starter-grid&#34;&gt;&#xD;&#xA;  &#xD;&#xA;  &lt;button class=&#34;video-btn&#34; data-video-id=&#34;DImp4PG2Ozs&#34;&gt;&#xD;&#xA;    &lt;img src=&#34;https://img.youtube.com/vi/DImp4PG2Ozs/maxresdefault.jpg&#34; alt=&#34;Episode 1&#34;&gt;&#xD;&#xA;    &lt;div&gt;&lt;h5&gt;Episode 1&lt;/h5&gt;&lt;/div&gt;&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;&#xD;&#xA;  &lt;button class=&#34;video-btn&#34; data-video-id=&#34;cyrcNVNpihI&#34;&gt;&#xD;&#xA;    &lt;img src=&#34;https://img.youtube.com/vi/cyrcNVNpihI/maxresdefault.jpg&#34; alt=&#34;Episode 2&#34;&gt;&#xD;&#xA;    &lt;div&gt;&lt;h5&gt;Episode 2&lt;/h5&gt;&lt;/div&gt;&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;&#xD;&#xA;  &lt;button class=&#34;video-btn&#34; data-video-id=&#34;whWfJ5GSQCE&#34;&gt;&#xD;&#xA;    &lt;img src=&#34;https://img.youtube.com/vi/whWfJ5GSQCE/maxresdefault.jpg&#34; alt=&#34;Episode 3&#34;&gt;&#xD;&#xA;    &lt;div&gt;&lt;h5&gt;Episode 3&lt;/h5&gt;&lt;/div&gt;&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;&#xD;&#xA;  &lt;button class=&#34;video-btn&#34; data-video-id=&#34;AkpDlqEwBoo&#34;&gt;&#xD;&#xA;    &lt;img src=&#34;https://img.youtube.com/vi/AkpDlqEwBoo/maxresdefault.jpg&#34; alt=&#34;Episode 4&#34;&gt;&#xD;&#xA;    &lt;div&gt;&lt;h5&gt;Episode 4&lt;/h5&gt;&lt;/div&gt;&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;&#xD;&#xA;  &lt;button class=&#34;video-btn&#34; data-video-id=&#34;_XcEVx5WJVg&#34;&gt;&#xD;&#xA;    &lt;img src=&#34;https://img.youtube.com/vi/_XcEVx5WJVg/maxresdefault.jpg&#34; alt=&#34;Episode 5&#34;&gt;&#xD;&#xA;    &lt;div&gt;&lt;h5&gt;Episode 5&lt;/h5&gt;&lt;/div&gt;&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;&#xD;&#xA;  &lt;button class=&#34;video-btn&#34; data-video-id=&#34;i4CLe5CNUso&#34;&gt;&#xD;&#xA;    &lt;img src=&#34;https://img.youtube.com/vi/i4CLe5CNUso/maxresdefault.jpg&#34; alt=&#34;Episode 6&#34;&gt;&#xD;&#xA;    &lt;div&gt;&lt;h5&gt;Episode 6&lt;/h5&gt;&lt;/div&gt;&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;&#xD;&#xA;  &lt;button class=&#34;video-btn&#34; data-video-id=&#34;94fLHdYXgNk&#34;&gt;&#xD;&#xA;    &lt;img src=&#34;https://img.youtube.com/vi/94fLHdYXgNk/maxresdefault.jpg&#34; alt=&#34;Episode 7&#34;&gt;&#xD;&#xA;    &lt;div&gt;&lt;h5&gt;Episode 7&lt;/h5&gt;&lt;/div&gt;&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;&#xD;&#xA;  &lt;button class=&#34;video-btn&#34; data-video-id=&#34;mCZw2LhMkBM&#34;&gt;&#xD;&#xA;    &lt;img src=&#34;https://img.youtube.com/vi/mCZw2LhMkBM/maxresdefault.jpg&#34; alt=&#34;Episode 8&#34;&gt;&#xD;&#xA;    &lt;div&gt;&lt;h5&gt;Episode 8&lt;/h5&gt;&lt;/div&gt;&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;&#xD;&#xA;  &lt;button class=&#34;video-btn&#34; data-video-id=&#34;ozWvFP-vXNY&#34;&gt;&#xD;&#xA;    &lt;img src=&#34;https://img.youtube.com/vi/ozWvFP-vXNY/maxresdefault.jpg&#34; alt=&#34;Episode 9&#34;&gt;&#xD;&#xA;    &lt;div&gt;&lt;h5&gt;Episode 9&lt;/h5&gt;&lt;/div&gt;&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;&#xD;&#xA;  &lt;button class=&#34;video-btn&#34; data-video-id=&#34;Tv5z7N5hYgw&#34;&gt;&#xD;&#xA;    &lt;img src=&#34;https://img.youtube.com/vi/Tv5z7N5hYgw/maxresdefault.jpg&#34; alt=&#34;Episode 10&#34;&gt;&#xD;&#xA;    &lt;div&gt;&lt;h5&gt;Episode 10&lt;/h5&gt;&lt;/div&gt;&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;&#xD;&#xA;  &lt;button class=&#34;video-btn&#34; data-video-id=&#34;z8-7T1hBnL8&#34;&gt;&#xD;&#xA;    &lt;img src=&#34;https://img.youtube.com/vi/z8-7T1hBnL8/maxresdefault.jpg&#34; alt=&#34;Episode 11&#34;&gt;&#xD;&#xA;    &lt;div&gt;&lt;h5&gt;Episode 11&lt;/h5&gt;&lt;/div&gt;&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;&#xD;&#xA;  &lt;div class=&#34;video-btn&#34; data-video-id=&#34;01010011010011110100111101001110&#34;&gt;&#xD;&#xA;    &lt;div style=&#34;aspect-ratio: 16/9; display: flex; align-items: center; justify-content: center; background: #444; color: #ddd; font-size: 1.5rem; letter-spacing: 2px;&#34;&gt;&#xD;&#xA;      &lt;strong&gt;???&lt;/strong&gt;&#xD;&#xA;    &lt;/div&gt;&#xD;&#xA;    &lt;div&gt;&lt;h5&gt;Episode 12&lt;/h5&gt;&lt;/div&gt;&#xD;&#xA;  &lt;/div&gt;&#xD;&#xA;&#xD;&#xA;&lt;/div&gt;&#xD;&#xA;&#xD;&#xA;&lt;script&gt;&#xD;&#xA;  document.querySelectorAll(&#39;.video-btn&#39;).forEach(btn =&gt; {&#xD;&#xA;    btn.addEventListener(&#39;click&#39;, function() {&#xD;&#xA;      const videoId = this.getAttribute(&#39;data-video-id&#39;);&#xD;&#xA;      const iframe = document.getElementById(&#39;mainVideo&#39;);&#xD;&#xA;      &#xD;&#xA;      // Change the video source&#xD;&#xA;      iframe.src = `https://www.youtube.com/embed/${videoId}?autoplay=1`;&#xD;&#xA;      &#xD;&#xA;      // Smooth scroll back to the big player&#xD;&#xA;      document.getElementById(&#39;bigPlayer&#39;).scrollIntoView({&#xD;&#xA;        behavior: &#39;smooth&#39;,&#xD;&#xA;        block: &#39;center&#39;&#xD;&#xA;      });&#xD;&#xA;    });&#xD;&#xA;  });&#xD;&#xA;&lt;/script&gt;</description>
    </item>
    <item>
      <title></title>
      <link>/startersquad/startersquad02/</link>
      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      <guid>/startersquad/startersquad02/</guid>
      <description>&lt;style&gt;&#xD;&#xA;  /* Theme-safe grid layout */&#xD;&#xA;  .music-grid {&#xD;&#xA;    display: grid;&#xD;&#xA;    grid-template-columns: repeat(1, 1fr);&#xD;&#xA;    gap: 1.5rem;&#xD;&#xA;    margin-top: 2rem;&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  @media (min-width: 768px) {&#xD;&#xA;    .music-grid { grid-template-columns: repeat(2, 1fr); }&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  @media (min-width: 1024px) {&#xD;&#xA;    .music-grid { grid-template-columns: repeat(4, 1fr); }&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  .video-btn {&#xD;&#xA;    width: 100%;&#xD;&#xA;    border: 0;&#xD;&#xA;    padding: 0;&#xD;&#xA;    cursor: pointer;&#xD;&#xA;    border-radius: 8px;&#xD;&#xA;    overflow: hidden;&#xD;&#xA;    background: #212529; /* Dark background to match theme */&#xD;&#xA;    box-shadow: 0 4px 6px rgba(0,0,0,0.3);&#xD;&#xA;    transition: transform 0.2s;&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  .video-btn:hover {&#xD;&#xA;    transform: scale(1.02);&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  .video-btn img {&#xD;&#xA;    width: 100%;&#xD;&#xA;    display: block;&#xD;&#xA;    height: auto;&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  .video-btn div {&#xD;&#xA;    padding: 1rem;&#xD;&#xA;    text-align: left;&#xD;&#xA;    color: white;&#xD;&#xA;  }&#xD;&#xA;&#xD;&#xA;  .video-btn h5 {&#xD;&#xA;    margin: 0;&#xD;&#xA;    font-size: 1rem;&#xD;&#xA;    font-weight: 500;&#xD;&#xA;  }&#xD;&#xA;&lt;/style&gt;&#xD;&#xA;&#xD;&#xA;&lt;h3 class=&#34;mb-4&#34;&gt;MUSIC&lt;/h3&gt;&#xD;&#xA;&lt;div class=&#34;music-grid&#34;&gt;&#xD;&#xA;  &#xD;&#xA;  &lt;button class=&#34;video-btn&#34; data-video-id=&#34;M8qyHTT3ku8&#34;&gt;&#xD;&#xA;    &lt;img src=&#34;https://img.youtube.com/vi/M8qyHTT3ku8/maxresdefault.jpg&#34; alt=&#34;Charmander&#39;s Lament&#34;&gt;&#xD;&#xA;    &lt;div&gt;&lt;h5&gt;Charmander&#39;s Lament&lt;/h5&gt;&lt;/div&gt;&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;&#xD;&#xA;  &lt;button class=&#34;video-btn&#34; data-video-id=&#34;7XGHN5U3Voc&#34;&gt;&#xD;&#xA;    &lt;img src=&#34;https://img.youtube.com/vi/7XGHN5U3Voc/maxresdefault.jpg&#34; alt=&#34;ROCK SMASHER&#34;&gt;&#xD;&#xA;    &lt;div&gt;&lt;h5&gt;Rock Smasher&lt;/h5&gt;&lt;/div&gt;&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;&#xD;&#xA;  &lt;button class=&#34;video-btn&#34; data-video-id=&#34;ubo_kxkabpM&#34;&gt;&#xD;&#xA;    &lt;img src=&#34;https://img.youtube.com/vi/ubo_kxkabpM/maxresdefault.jpg&#34; alt=&#34;Squad Song&#34;&gt;&#xD;&#xA;    &lt;div&gt;&lt;h5&gt;Squad Song&lt;/h5&gt;&lt;/div&gt;&#xD;&#xA;  &lt;/button&gt;&#xD;&#xA;&#xD;&#xA;&lt;/div&gt;&#xD;&#xA;&#xD;&#xA;&lt;script&gt;&#xD;&#xA;  document.querySelectorAll(&#39;.video-btn&#39;).forEach(btn =&gt; {&#xD;&#xA;    btn.addEventListener(&#39;click&#39;, function() {&#xD;&#xA;      const videoId = this.getAttribute(&#39;data-video-id&#39;);&#xD;&#xA;      const iframe = document.getElementById(&#39;mainVideo&#39;);&#xD;&#xA;      &#xD;&#xA;      // Change the video source&#xD;&#xA;      iframe.src = `https://www.youtube.com/embed/${videoId}?autoplay=1`;&#xD;&#xA;      &#xD;&#xA;      // Smooth scroll back to the big player&#xD;&#xA;      document.getElementById(&#39;bigPlayer&#39;).scrollIntoView({&#xD;&#xA;        behavior: &#39;smooth&#39;,&#xD;&#xA;        block: &#39;center&#39;&#xD;&#xA;      });&#xD;&#xA;    });&#xD;&#xA;  });&#xD;&#xA;&lt;/script&gt;</description>
    </item>
    <item>
      <title></title>
